<h1>Listing users_statuses</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users_statuses.each do |users_status| %>
  <tr>
    <td><%= users_status.name %></td>
    <td><%= link_to 'Show', users_status %></td>
    <td><%= link_to 'Edit', edit_users_status_path(users_status) %></td>
    <td><%= link_to 'Destroy', users_status, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

